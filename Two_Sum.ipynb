      },
      "source": [
        "def twoSum(nums , target):\n",
        "  n = len(nums)\n",
        "  h = {}\n",
        "  for i in range(0, n):\n",
        "    x = target - nums[i]\n",
        "    if x in h:\n",
        "      return (h[x] , i)\n",
        "    h[nums[i]] = i"
      ],
      "execution_count": 1,
      "outputs": []
    },
